<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <!-- Google Font -->
    <link href="https://fonts.googleapis.com/css2?family=Proza+Libre:ital,wght@0,500;1,500&family=Vollkorn:ital,wght@0,700;1,700&display=swap" rel="stylesheet">
    <!-- Font Awesome -->
     <script src="https://kit.fontawesome.com/3c79b583ac.js" crossorigin="anonymous"></script>
    <!-- TinySlider -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/tiny-slider/2.9.3/tiny-slider.css">
    <!-- DrawSVG -->
    <script src="js/gsap/DrawSVGPlugin.min.js"></script>
    <!-- ScrollTrigger -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.4.2/ScrollTrigger.min.js"></script>
    <!-- Greensock Scroll To Plugin -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.4.2/ScrollToPlugin.min.js"></script>
     <!-- Typed --> 
     <script src="js/typed/typed.js"></script>


    <link rel="stylesheet" href="css/styles.css">
    <title>Why Camping?</title>
</head>

<body>
    <!-- @import "header.kit" -->
    <!-- @import "main-nav.kit" -->
    <!-- @import "section-1.kit" -->
    <!-- @import "section-2.kit" -->
    <!-- @import "section-4.kit" -->
    <!-- @import "section-3.kit" -->
    <!-- @import "section-5.kit" -->
    <!-- @import "section-6.kit" -->
    <!-- @import "footer.kit" -->
 

    <script>
        function initMap() {

            var map = new google.maps.Map(document.getElementById('map'), {
                center: {
                    lat: 40.215849,
                    lng: -75.784505
                },
                zoom: 13,

                // change to hybrid satellite view
                mapTypeId: 'hybrid',

                styles: [
                    // change to dark green geometry
                    {
                        elementType: 'geometry',
                        stylers: [{
                            color: '#58624A'
                        }]
                    },
                     // change to black text stroke
                    {
                        elementType: 'labels.text.stroke',
                        stylers: [{
                            color: '#000000'
                        }]
                    },
                    // change to light-green text fill
                    {
                        elementType: 'labels.text.fill',
                        stylers: [{
                            color: '#A7B9AE'
                        }]
                    },
                    {
                    // change to light-green
                        featureType: 'poi',
                        elementType: 'labels.text.fill',
                        stylers: [{
                            color: '#A7B9AE',
                        }]
                    },
                    {
                    // change to dark-green
                        featureType: 'poi.park',
                        elementType: 'geometry',
                        stylers: [{
                            color: '#173522'
                        }]
                    },
                    {
                    // change to orange
                        featureType: 'poi.park',
                        elementType: 'labels.text.fill',
                        stylers: [{
                            color: '#E86F11'
                        }]
                    },
                    {
                    // change to white
                        featureType: 'road',
                        elementType: 'labels.text.fill',
                        stylers: [{
                            color: '#ffffff'
                        }]
                    },
                    {
                        // change to dark-blue
                        featureType: 'water',
                        elementType: 'geometry',
                        stylers: [{
                            color: '#2A5670'
                        }]
                    },
                ]
            });

            var myLatLng = {
                lat: 40.215849,
                lng: -75.784505
            };

            var marker = new google.maps.Marker({
                position: myLatLng,
                map: map,
                draggable: true,
                animation: google.maps.Animation.DROP,
                title: "Hello!"
            });

            marker.addListener("click", toggleBounce);

            function toggleBounce() {
                if (marker.getAnimation() !== null) {
                    marker.setAnimation(null);
                } else {
                    marker.setAnimation(google.maps.Animation.BOUNCE);
                }
            }
        }
    </script>
    <script async defer
        src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCrgc38FmWsOp9oGyoXorMzh62whWUmZk0&callback=initMap">
    </script>

    
    <!-- jQuery -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <!-- Greensock -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.5.0/gsap.min.js"></script>
    <!-- ScrollTrigger -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.4.2/ScrollTrigger.min.js"></script>
    <!-- TinySlider -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/tiny-slider/2.9.2/min/tiny-slider.js"></script>
   

    <script src="js/scripts-min.js"></script>
</body>

</html>